#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/owl/mcpRCH/node_modules/.pnpm/@colyseus+schema@3.0.39/node_modules/@colyseus/schema/bin/node_modules:/Users/owl/mcpRCH/node_modules/.pnpm/@colyseus+schema@3.0.39/node_modules/@colyseus/schema/node_modules:/Users/owl/mcpRCH/node_modules/.pnpm/@colyseus+schema@3.0.39/node_modules/@colyseus/node_modules:/Users/owl/mcpRCH/node_modules/.pnpm/@colyseus+schema@3.0.39/node_modules:/Users/owl/mcpRCH/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/owl/mcpRCH/node_modules/.pnpm/@colyseus+schema@3.0.39/node_modules/@colyseus/schema/bin/node_modules:/Users/owl/mcpRCH/node_modules/.pnpm/@colyseus+schema@3.0.39/node_modules/@colyseus/schema/node_modules:/Users/owl/mcpRCH/node_modules/.pnpm/@colyseus+schema@3.0.39/node_modules/@colyseus/node_modules:/Users/owl/mcpRCH/node_modules/.pnpm/@colyseus+schema@3.0.39/node_modules:/Users/owl/mcpRCH/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/schema-codegen" "$@"
else
  exec node  "$basedir/../../bin/schema-codegen" "$@"
fi
